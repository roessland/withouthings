{{define "title"}}Measure - Getmeas{{end}}

{{define "content"}}
  <div class="py-4">
    <div class="">
        {{if not .GetmeasResponseBody}}
          <p>No data</p>
          <p>{{ .Error }}</p>
        {{else}}
          <pre>
            <code>
              {{ printf "%s" .GetmeasResponseBody}}
            </code>
          </pre>
        {{end}}
    </div>
  </div>
{{end}}
{{define "title"}}Notifications{{end}}

{{define "content"}}
  <div class="py-4">
    <div class="">
        {{if not .Notifications}}
          <p>No data</p>
            <p>{{ .Error }}</p>
        {{else}}
          <ul data-test-id="notifications-list-ul">
              {{ range $_, $value := .Notifications }}
                <li>
                  <strong>{{ $value }}</strong>:
                  <div class="sleepscore-hbar"
                       style="width: px;">
                  </div>
                  <i>$value.Params</i>

                </li>
              {{ end }}
          </ul>

          <pre>
        <code>
        {{ printf "%s" .Notifications }}
        </code>
    </pre>

        {{end}}
    </div>
  </div>
{{end}}